{
  "name": "base-react-next-v2",
  "private": true,
  "engines": {
    "yarn": ">=1.22.19",
    "node": ">=16.15"
  },
  "scripts": {
    "prepare": "is-ci || husky install",
    "dev": "next dev --port 3000",
    "build": "next build && next export",
    "build-clean": "rimraf .next out",
    "start": "next start",
    "lint-es": "eslint --ext \".ts,.tsx,.js\" --max-warnings 0 .",
    "prettier-check": "prettier --check \"**/*.{yml,md,json}\"",
    "lint": "npm-run-all --print-label --parallel lint-es prettier-check",
    "minify-svg": "yarn svgo --folder src/components/icons/svg && yarn svgo --folder public",
    "start-storybook": "start-storybook --port 6006",
    "build-storybook": "build-storybook",
    "build-storybook-clean": "rimraf storybook-static",
    "test-unit": "jest",
    "test-unit-coverage": "jest --coverage",
    "test-unit-ci": "jest --ci",
    "test-unit-clean": "yarn jest --clearCache && yarn rimraf coverage",
    "clean-all": "npm-run-all --print-label --parallel *-clean && yarn rimraf node_modules"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@hookform/resolvers": "2.9.10",
    "@mui/material": "5.10.15",
    "@mui/x-date-pickers": "5.0.8",
    "@react-google-maps/api": "2.17.0",
    "axios": "1.2.0",
    "clsx": "1.2.1",
    "dayjs": "1.11.6",
    "immer": "9.0.16",
    "next": "13.0.4",
    "nprogress": "0.2.0",
    "react": "18.2.0",
    "react-beautiful-dnd": "13.1.1",
    "react-dom": "18.2.0",
    "react-hook-form": "7.39.5",
    "react-window": "1.8.8",
    "swr": "1.3.0",
    "zod": "3.19.1",
    "zustand": "4.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@commitlint/cli": "17.3.0",
    "@commitlint/config-conventional": "17.3.0",
    "@next/eslint-plugin-next": "13.0.4",
    "@storybook/addon-actions": "6.5.13",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-interactions": "6.5.13",
    "@storybook/addon-links": "6.5.13",
    "@storybook/builder-webpack5": "6.5.13",
    "@storybook/manager-webpack5": "6.5.13",
    "@storybook/react": "6.5.13",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.11.9",
    "@types/nprogress": "0.2.0",
    "@types/react": "18.0.25",
    "@types/react-beautiful-dnd": "13.1.2",
    "@types/react-dom": "18.0.9",
    "@types/react-window": "1.8.5",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "babel-loader": "8.3.0",
    "cypress": "11.2.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-cypress": "2.12.1",
